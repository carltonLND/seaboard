{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rimraf ./node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "sync": "npx syncpack fix-mismatches && pnpm install"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "syncpack": "^11.2.1",
    "tsconfig": "workspace:*",
    "turbo": "latest",
    "rimraf": "^3.0.2"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "seaboard"
}
